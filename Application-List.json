        },
        {
            "Name": "Azure Service Fabric SDK",
            "Message": "Installing Azure Service Fabric SDK",
            "Script": "webpicmd /install /products:'Microsoft Azure Service Fabric SDK and Tools - 2.4.145 (VS2015)' /AcceptEULA /SuppressReboot"
