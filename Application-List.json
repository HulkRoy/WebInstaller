        {
            "Name": "Aws SDK",
            "Message": "Installing Aws SDK",
            "Script": "Start-Process -FilePath msiexec.exe -ArgumentList '/i https://sdk-for-net.amazonwebservices.com/latest/AWSToolsAndSDKForNet.msi /quiet /norestart' -wait"
