{
  "name": "batch-installer",
  "version": "1.0.0",
  "description": "A tool for batch install windows application",
  "scripts": {
    "build": "npm install && typings install",
    "start": "npm run serve",
    "serve": "webpack --config config/webpack.dev.js --progress && node server/server.js",
    "serve:prod": "webpack --config config/webpack.prod.js --progress && node server/server.js"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "raw-loader": "^0.5.1",
    "to-string-loader": "^1.1.5",
    "tslint": "^4.3.1",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.4",
    "typings": "^2.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "dependencies": {
    "@angular/common": "^2.4.2",
    "@angular/compiler": "^2.4.3",
    "@angular/core": "^2.4.2",
    "@angular/forms": "^2.4.4",
    "@angular/http": "^2.4.2",
    "@angular/platform-browser": "^2.4.3",
    "@angular/platform-browser-dynamic": "^2.4.2",
    "@angular/router": "^3.4.3",
    "bootstrap": "^3.3.7",
    "clipboard": "^1.5.16",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.2"
  }
}
